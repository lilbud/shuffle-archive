r": [{"embeddable": true, "href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/users/1"}], "replies": [{"embeddable": true, "href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/comments?post=1734"}], "version-history": [{"count": 12, "href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/posts/1734/revisions"}], "predecessor-version": [{"id": 56561, "href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/posts/1734/revisions/56561"}], "wp:featuredmedia": [{"embeddable": true, "href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/media/26621"}], "wp:attachment": [{"href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/media?parent=1734"}], "wp:term": [{"taxonomy": "category", "embeddable": true, "href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/categories?post=1734"}, {"taxonomy": "post_tag", "embeddable": true, "href": "https://estreetshuffle.com/index.php/wp-json/wp/v2/tags?post=1734"}], "curies": [{"name": "wp", "href": "https://api.w.org/{rel}", "templated": true}]}}